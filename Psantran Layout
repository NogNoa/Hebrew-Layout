partial alphanumeric_keys
xkb_symbols "sfr" {
    name[Group1]= "Hebrew (Shofar)";

    //Similar to but not decended from SI 1452 of the Standards Institution of Israel
    //uses Hebrew (lyx) as baseline.

    key <TLDE> { [ hebrew_finalzade,	asciitilde	]	};
    key <AD01> { [ hebrew_finalpe, slash	]       };
    key <AD02> { [ hebrew_taw,	 apostrophe	]       };
    key <AD03> { [ hebrew_qoph,	 0x10005b8	]	}; // Qamats
